{
  "name": "heia-backend",
  "version": "1.0.0",
  "description": "Heia blog API",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src --out-dir ./build",
    "start": "node build/index.js",
    "dev": "env-cmd -f ./config/dev.env nodemon --exec babel-node src/index.js",
    "test": "env-cmd -f ./config/test.env jest --runInBand --verbose --coverage",
    "dev:test": "env-cmd -f ./config/test.env jest --verbose --runInBand",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CohortOnePointOne/Heai-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CohortOnePointOne/Heai-backend/issues"
  },
  "homepage": "https://github.com/CohortOnePointOne/Heai-backend#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "dotenv": "^16.0.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "mongoose": "^6.7.2",
    "nodemon": "^2.0.20"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/node": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/runtime": "^7.20.1",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-blueimp": "^2.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "prettier": "^2.7.1",
    "supertest": "^6.3.1"
  }
}
